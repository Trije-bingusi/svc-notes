generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Note {
  id         String   @id @default(uuid()) @db.Uuid
  lecture_id String   @db.Uuid
  user_id    String?  @db.Uuid
  content    String
  created_at DateTime @default(now()) @db.Timestamptz(6)
}
